doctype

html(lang="ru")
  
  -var active = 'Контакты' //активный пункт меню, список в header.jade
  -var pageTitle = 'Контакты' + ' - Телефон03' //title страницы для генерации верстки  

  include ./_include/_head

  body
    
    // Preloader
    include ./_include/_preloader
    
    include ./_include/_header
    
    .breadcrumbs
      .container
        
        ul.breadcrumbs__list
          li
            a(href="#!") Телефон 03
          li
            span Название страницы
    
    .content
      .container
        
        .content__head
          h1 Выездной ремонт телефонов
          .content__head-untitle Приедем уже сегодня и починим в вашем присутствии! 
        
        .contact
          .contact__wrapper
            div.hidden-xs
              img.contact__img(src="./img/contact-img.png", alt="")
            div
              .contact__body-one
                .contact__block
                  img(src="./img/contacts-img-1.svg", alt="")
                  a(href="#!") 8 (123) 456-78-90
                  span Звоните круглосуточно
                .contact__block
                  img(src="./img/contacts-img-2.svg", alt="")
                  a(href="#!") 8 (987) 654-43-21
                  span Наш WhatsApp
                  
                .contact__block
                  img(src="./img/contacts-img-3.svg", alt="")
                  a(href="#!") 8 (123) 456-78-90
                  span Наш Viber
                  
                .contact__block
                  img(src="./img/contacts-img-4.svg", alt="")
                  a(href="#!") 8 (123) 456-78-90
                  span Наш Telegram
            div
              .contact__body-two
                .contact__block
                  img(src="./img/contacts-img-5.svg", alt="")
                  a(href="javascript:void(0)") с 10:00 до 20:00
                  span Без выходных и праздников
                
                .clear10
                .contact__social
                  a(href="#!")
                    img(src="./img/contact-social-1.png", alt="")
                .contact__social
                  a(href="#!")
                    img(src="./img/contact-social-2.png", alt="")

    include ./_include/_wave

    
    include ./_include/_footer
    //- div(style="height: 1000px; background-color: #000")
    
    //[if gte IE 9]
      <script src='./js/libs/html5shiv/es5-shim.min.js'>
      <script src='./js/libs/html5shiv/html5shiv.min.js'>
      <script src='./js/libs/html5shiv/html5shiv-printshiv.min.js'>
      <script src='./js/libs/html5shiv/respond.min.js'><![endif]

    // Load Scripts
    script(src="./js/libs.js")
    include ./_include/_footer-js
    script(src="./js/common.js")
    
